QtWidgets:QWidget{
    background-color:#DDB1B1;
    border:none;
    border-radius:15px;
}
#login {
    background-color: #E9E9E9;
    border-radius: 10px;
    color: #07C160;
}
#login:hover {
    background-color: #D2D2D2;
}
#login:pressed {
    background-color: #C6C6C6;
}
#pushButton_ren{
    background-color: #D98759;
    border-radius: 5px;
    color: #FFFFFF;
}
#pushButton_ren:hover {
    background-color: #E1A37E;
}

#pushButton_ren:pressed {
    background-color:  #D98759;
}